#Main Database Configuration

main.db.driver-class-name= com.mysql.cj.jdbc.Driver
main.db.url= jdbc:mysql://localhost:3306/ebs
#jdbc = API
#mysql = database
#localhost = servername
#3306 = port number
main.db.username= root
main.db.password= root
#Hibernate Configurations
spring.jpa.show-sql= true
spring.jpa.ddl-auto = update
spring.jpa.generate-ddl=true
spring.jpa.hibernate.ddl-auto = update
spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.MySQL5InnoDBDialect


#Second Database Connection
second.db.driver=om.mysql.cj.jdbc.Driver
second.db.url=jdbc:mysql://localhost:3306/test1
second.db.username=root
second.db.password=root

#Third Database Connection
third.db.driver=om.mysql.cj.jdbc.Driver
third.db.url=jdbc:mysql://localhost:3306/test2
third.db.username=root
third.db.password=root